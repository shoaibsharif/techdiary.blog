{
    "name": "techdiary.blog",
    "version": "1.0.0",
    "scripts": {
        "start": "env-cmd -f env/.prod.env node build/server.js",
        "frontend-dev": "parcel watch ./app/index.js --out-dir ./public/dist",
        "frontend-build": "parcel build ./app/index.js --out-dir ./public/dist",
        "server-dev": "env-cmd -f env/.dev.env nodemon --exec babel-node src/server.js",
        "server-build": "babel src --out-dir build",
        "dev": "concurrently \"npm run server-dev\" \"npm run frontend\"",
        "build": "concurrently \"npm run server-build\" \"npm run frontend-build\"",
        "api-doc": "apidoc -i src -o docs",
        "postinstall": "npm run build"
    },
    "keywords": [],
    "author": {
        "name": "techdiary-blog",
        "url": "https://techdiary.blog"
    },
    "license": "ISC",
    "dependencies": {
        "axios": "^0.19.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.4",
        "env-cmd": "^10.0.1",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.8.1",
        "mongoose-unique-validator": "^2.0.3",
        "nodemailer": "^6.4.1",
        "uuid": "^3.3.3",
        "validator": "^12.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.5",
        "@babel/core": "^7.7.5",
        "@babel/node": "^7.7.4",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@babel/preset-env": "^7.7.5",
        "@babel/preset-react": "^7.7.4",
        "@hapi/joi": "^16.1.8",
        "apidoc": "^0.19.0",
        "babel-eslint": "^10.0.3",
        "babel-plugin-module-resolver": "^4.0.0",
        "concurrently": "^5.0.2",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.1",
        "morgan": "^1.9.1",
        "nodemon": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2"
    }
}
